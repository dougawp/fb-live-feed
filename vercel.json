{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Frame-Options", "value": "" },
        { "key": "Content-Security-Policy", "value": "frame-ancestors https://*.canva.com https://*.canva.site" },
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    }
  ]
}
